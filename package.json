{
  "name": "beefy-proto",
  "version": "0.5.0",
  "private": true,
  "dependencies": {
    "@chainlink/contracts": "^0.2.1",
    "@nomiclabs/hardhat-etherscan": "^2.1.4",
    "@openzeppelin-2/contracts": "npm:@openzeppelin/contracts@2.5.1",
    "@openzeppelin-4/contracts": "npm:@openzeppelin/contracts",
    "@openzeppelin/contracts": "^3.2.0",
    "@openzeppelin/contracts-upgradeable": "^4.3.2",
    "@uniswap/lib": "^4.0.1-alpha",
    "bignumber.js": "^9.0.1",
    "blockchain-addressbook": "^0.3.17",
    "ganache-cli": "^6.12.2",
    "keccak": "^3.0.1",
    "rlp": "^2.2.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "scripts": {
    "flat-hardhat": "hardhat flatten contracts/BIFI/strategies/common/StrategyCommonRewardPoolSingle.sol > tmp/flattened.sol",
    "flat": "bin/flatten.sh",
    "clean": "rm -r artifacts cache",
    "net": "hardhat node --fork",
    "net:bsc": "yarn net bsc",
    "compile": "hardhat compile",
    "all": "yarn clean && yarn compile && yarn net",
    "test": "hardhat test --network localhost",
    "test:vault": "hardhat test --network localhost test/prod/VaultLifecycle.test.js",
    "deploy-strat:bsc": "hardhat run --network bsc scripts/deploy-strat-with-pool.js",
    "deploy:lend": "hardhat run scripts/deploy-strat-lend.js --network",
    "deploy-strat:polygon": "hardhat run --network polygon scripts/deploy-strat-with-pool.js",
    "deploy:rewardPool": "hardhat run scripts/deploy-generic-rewardPool-strat.js --network",
    "deploy:chef": "hardhat run scripts/deploy-generic-chef-strat.js --network",
    "deploy-pool:bsc": "hardhat run --network bsc scripts/deploy-launchpad-pool.js",
    "notify:bsc": "hardhat run --network bsc scripts/notify-launchpools.js",
    "abi-to-sol": "abi-to-sol"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/hardhat-upgrades": "^1.10.0",
    "@openzeppelin/upgrades-core": "^1.9.2",
    "abi-to-sol": "^0.2.1",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.31",
    "hardhat": "^2.6.4",
    "mocha": "^9.1.1",
    "truffle-flattener": "^1.5.0",
    "web3": "^1.3.0"
  }
}
